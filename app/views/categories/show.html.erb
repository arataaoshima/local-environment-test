<p id="notice"><%= notice %></p>

<h1>
  
  <%= @category.title %>
</h1>
<p>
  <% @category.posts.each do |post| %>
   <%= link_to post.title, post_path(post), class:"btn btn-outline-dark" %>
  <% end %>
</p>
<%= link_to 'Edit', edit_category_path(@category), class:"btn btn-outline-dark"  %> |
<%= link_to 'Back', categories_path, class:"btn btn-outline-dark"  %>
